<%@ val userID : String %>
<%@ val keyInfoList : List[dsmoq.apikeyweb.KeyInfo] %>

<h2>ユーザーID：${userID}</h2>
<table>
    <tr>
        <th>Consumer Key</th>
        <th>Secret Key</th>
    </tr>
    #for (keyInfo <- keyInfoList)
        <tr>
        <td>${keyInfo.consumerKey}</td>
        <td>${keyInfo.secretKey}</td>
        </tr>
    #end
</table>
<hr/>
<a href="/">Back</a>
